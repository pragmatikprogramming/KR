<table class="table">
    <thead>
        <tr>
            <td style="font-weight: bold;">Name:</td>
            <td width="13px;"></td>
            <td width="13px;"></td>
            <td width="13px;"></td>
            <td width="13px;"></td>
        </tr>
    </thead>
    @foreach (var staff in Model)
    {
        <tr id="compList" style width="100%">
            <td><a href="/Staff/DisplayStaff/@staff.Id">@staff.LastName, @staff.FirstName</a></td>
            <td><a href="#"><i class="icon-envelope" title="Send Email" alt="Send Email"></i></a></td>
            <td><a href="/Note/AddNote/@staff.Id/Staff"><i class="icon-comment" title="Add Note" alt="Add Note"></i></a></td>
            <td><a href="/Staff/EditStaff/@staff.Id"><i class="icon-pencil" title="Edit" alt="Edit"></i></a></td>
            <td><a href="/Staff/DeleteStaff/@staff.Id/@ViewBag.PageNumber/0/@ViewBag.Filter"><i class="icon-remove" title="Delete" alt="Delete"></i></a></td>
        </tr>
    }
    <tr>
        <td colspan="3">
        @if (ViewBag.PaginationStart > 1)
        {
            @:...&nbsp;&nbsp;
        }
        @for (var i = ViewBag.PaginationStart; i <= ViewBag.PaginationEnd; i++)
        {
            <a href="/Staff/SearchStaff/@i/0/@ViewBag.Filter">@i &nbsp;&nbsp;&nbsp;</a>
        }
        @if(ViewBag.PaginationEnd < ViewBag.NumPages)
        {
            @: ...
        }
        </td>
    </tr>
</table>
